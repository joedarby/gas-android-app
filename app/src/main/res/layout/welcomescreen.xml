<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/relativelayout2"
        android:layout_marginRight="51dp"
        android:layout_marginEnd="51dp"
        android:layout_marginBottom="32dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">

        <TextView
            android:layout_width="145dp"
            android:layout_height="wrap_content"
            android:text="TTF"
            android:id="@+id/textTTF"
            android:textAlignment="center"
            android:textSize="24dp"
            android:paddingBottom="10dp"/>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textTTF"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/dutchFlows">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/fire"
                android:id="@+id/imageView12" />

            <TextView
                android:text="Flows"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView10"
                android:textAlignment="center"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/dutchFlows"
            android:layout_toRightOf="@+id/dutchFlows"
            android:layout_below="@+id/textTTF"
            android:paddingLeft="10dp"
            android:paddingStart="10dp">

            <ImageView
                android:layout_width="60dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/profit"
                android:id="@+id/imageView13"
                android:scaleType="fitXY"
                android:adjustViewBounds="true" />

            <TextView
                android:text="Historical\nData"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView11"
                android:textAlignment="center"/>
        </LinearLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="200dp"
        android:id="@+id/relativelayout1"
        android:layout_marginRight="10dp"
        android:layout_marginEnd="10dp"
        android:layout_above="@+id/relativelayout2"
        android:layout_toLeftOf="@+id/relativelayout2"
        android:layout_toStartOf="@+id/relativelayout2">

        <TextView
            android:layout_width="145dp"
            android:layout_height="wrap_content"
            android:text="NBP"
            android:id="@+id/nbpText"
            android:textAlignment="center"
            android:textSize="24dp"
            android:paddingBottom="10dp"/>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:orientation="vertical"
            android:id="@+id/summaryAndFlows"
            android:layout_below="@+id/nbpText">

            <ImageView
                android:layout_width="75dp"
                android:layout_height="46dp"
                app:srcCompat="@drawable/arrows_green_red"
                android:id="@+id/imageSummaryArrows"
                android:scaleType="fitXY"
                android:adjustViewBounds="true"
                android:layout_gravity="center"/>

            <TextView
                android:text="TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView7"
                tools:text="Summary"
                android:textAlignment="center"
                android:layout_gravity="center" />



            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/fire"
                android:id="@+id/imageView8"
                android:scaleType="fitXY"
                android:adjustViewBounds="true"
                android:layout_gravity="center"
                android:paddingTop="10dp"/>

            <TextView
                android:text="TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView6"
                tools:text="Flows"
                android:textAlignment="center"
                android:layout_gravity="center"/>

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/summaryAndFlows"
            android:weightSum="1"
            android:paddingLeft="10dp">

            <ImageView
                android:layout_width="60dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/profit"
                android:id="@+id/imageView9"
                android:adjustViewBounds="true"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/summaryAndFlows" />

            <TextView
                android:text="Historical\nData"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView8"
                android:textAlignment="center"/>
        </LinearLayout>



    </RelativeLayout>

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/frameLayout2"
        android:layout_marginBottom="11dp"
        android:layout_above="@+id/relativelayout1"
        android:layout_alignRight="@+id/relativelayout2"
        android:layout_alignEnd="@+id/relativelayout2"
        android:layout_marginRight="10dp"
        android:layout_marginEnd="10dp">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <TextView
                android:text="Norway"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView12"
                android:paddingBottom="10dp"
                android:textSize="24dp"/>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/fire"
                android:id="@+id/imageView11" />

            <TextView
                android:text="Flows"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView9"
                android:textAlignment="center"/>

        </LinearLayout>
    </FrameLayout>


</RelativeLayout>