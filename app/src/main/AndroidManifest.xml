<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.darby.joe.gas">

<uses-permission android:name="android.permission.INTERNET"/>
<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme"
    android:name=".Tools.GasApplication">
    <activity android:name=".Activities.StartActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name=".Activities.NBPLinepackDataActivity"/>
    <activity android:name=".Activities.TerminalListActivity"/>
    <activity android:name=".Activities.TerminalDetailActivity"/>
    <activity android:name=".Activities.MultipleChartActivity"/>

</application>

</manifest>